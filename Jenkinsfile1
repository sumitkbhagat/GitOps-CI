pipeline {
    agent any
    tools{
        jdk  'jdk11'
        maven  'maven3' 
    }

    stages {
        stage('Git CheckOut') {
            steps {
                git branch: 'main', url: 'https://github.com/sumitkbhagat/GitOps-CI.git'
            }
        }
        stage('Maven Build') {
            steps {
                sh 'mvn clean package -DskipTests=true'
            }
        }
        
    }
}
